<resources>
    <string name="app_name">MVPDemo</string>

    <!--登录界面-->
    <string name="user_name">用户名</string>
    <string name="password">密码</string>
    <string name="login">登录</string>
    <string name="user_name_warning">用户名不正确!</string>
    <string name="password_warning">密码不正确!</string>

    <!--navigation-->
    <string name="repos">Repos</string>
    <string name="follow">Follow</string>
    <string name="settings">Settings</string>
    <string name="about">About</string>

    <string name="item_example_number_title">第%02d条数据</string>
    <string name="item_example_number_abstract">这是测试的第%02d条数据</string>

    <!--上传下载进度条-->
    <declare-styleable name="DownloadingProgressBar">
        <attr format="enum" name="bga_pb_mode">
            <enum name="system" value="0"/>
            <enum name="horizontal" value="1"/>
            <enum name="circle" value="2"/>
            <enum name="comet" value="3"/>
            <enum name="wave" value="4"/>
        </attr>
        <attr format="reference|color" name="bga_pb_textColor"/>
        <attr format="dimension" name="bga_pb_textSize"/>
        <attr format="dimension" name="bga_pb_textMargin"/>
        <attr format="reference|color" name="bga_pb_reachedColor"/>
        <attr format="dimension" name="bga_pb_reachedHeight"/>
        <attr format="reference|color" name="bga_pb_unReachedColor"/>
        <attr format="dimension" name="bga_pb_unReachedHeight"/>
        <attr format="boolean" name="bga_pb_isCapRounded"/>
        <attr format="boolean" name="bga_pb_isHiddenText"/>
        <attr format="dimension" name="bga_pb_radius"/>
    </declare-styleable>

    <!--权限获取-->
    <string name="permission_failed_title">权限申请失败</string>
    <string name="permission_failed_setting">去设置</string>
    <string name="permission_failed_cancel">取消</string>
    <string name="permission_always_failed_message">我们需要的一些权限被您拒绝，请您到设置页面手动授权，否则功能无法正常使用！\n\n%1$s</string>
    <string name="permission_install_failed_message">我们需要安装应用，请您到设置页面手动授权，否则功能无法正常使用！</string>



</resources>
